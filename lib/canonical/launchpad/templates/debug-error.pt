<html metal:use-macro="context/@@standard_macros/dialog">

    <!-- Using the Z3 standard template so we can see bugs with our
        customized templates -->

<head>
  <title metal:fill-slot="title">Launchpad - Error Debug Page</title>
</head>

<body>
<div metal:fill-slot="body">

  <h3>
    Error type: 
    <tal:span tal:replace="view/error_type" />
  </h3>

  <div tal:content="structure view/htmltext | default">
     HTML rendering of error should go here.
  </div>

</div>
</body>
</html>

