<div
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  class="portlet" id="portlet-lifecycle">
  <h2>Lifecycle</h2>
  <div class="portletBody portletContent">
    <table>
      <tbody>
        <tr>
          <th>Registered by:</th>
          <td>
            <a
              tal:attributes="href context/owner/fmt:url"
              tal:content="context/owner/displayname"
            />
          </td>
        </tr>
        <tr>
          <th>When:</th>
          <td tal:attributes="title context/date_created/fmt:datetime"
              tal:content="context/date_created/fmt:approximatedate">
            2006-05-14
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
