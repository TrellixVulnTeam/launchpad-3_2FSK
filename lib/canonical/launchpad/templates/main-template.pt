<metal:page define-macro="master"><metal:doctype define-slot="doctype"><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"></metal:doctype>
<metal:block define-slot="top_slot" />
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en"
    tal:attributes="lang default_language|default;
                    xml:lang default_language|default;">
<head>
    <title tal:content="CONTEXTS/fmt:pagetitle">Launchpad</title>
    <!-- Plone style sheet -->
    <style type="text/css"
        media="screen">@import url(/@@/plone.css);</style>
    <!-- Launchpad style sheet -->
    <style type="text/css"
        media="screen">@import url(/@@/launchpad.css);</style>
    <!-- Launchpad JS code -->
    <script type="text/javascript" src="/@@/launchpad.js"></script>
    <script type="text/javascript" src="/@@/sorttable.js"></script>
    <link rel="shortcut icon" href="/@@/defaultFavicon.gif" />

    <metal:block define-slot="head_epilogue"></metal:block>
</head>

  <body>
    <img
      style="position: relative; float: left; left: -12px; top: 1em;"
      src="/@@/launchpad.png"
      alt=""
    />
    <div class="header">
      <div
        class="login"
        tal:replace="structure context/@@login_status"
      >
        Logged in or logged out
      </div>
      <div
        class="hierarchy"
        tal:content="structure request/fmt:breadcrumbs"
      />
      <ul
        class="facets"
        tal:define="facetmenu context/menu:facet"
      >
        <tal:block condition="facetmenu">
          <tal:block repeat="link facetmenu">
            <li
              tal:condition="link/selected"
              class="current"
            >
              <strong tal:condition="link/linked">
                <a
                  tal:attributes="href link/url"
                  tal:content="link/text"
                >abc</a>
              </strong>
              <strong
                tal:condition="not: link/linked"
                tal:content="link/text"
              >def</strong>
            </li>
            <li tal:condition="not: link/selected">
              <a
                tal:attributes="href link/url"
                tal:content="link/text"
              >ghi</a>
            </li>
          </tal:block>
        </tal:block>
      </ul>
    </div>
    <h2><metal:block
        define-slot="titleheader"
        tal:content="context/title | default"
    >Launchpad</metal:block></h2>
    <ul
      tal:define="appmenu context/menu:application"
      tal:condition="appmenu"
      class="app"
    >
      <tal:block repeat="link appmenu">
        <li
          tal:condition="link/selected"
          class="current"
        >
          <tal:block condition="link/linked">
            <strong>
              <a 
                tal:attributes="href link/url"
                tal:content="link/text"
              >link text</a>
            </strong>
          </tal:block>
          <tal:block condition="not: link/linked">
            <strong tal:content="link/text">link text</strong>
          </tal:block>
        </li>
        <li tal:condition="not: link/selected">
          <a
            tal:attributes="href link/url"
            tal:content="link/text"
          >link text</a>
        </li>
      </tal:block>
    </ul>

    <!-- Menus -->


    <div class="visualClear" id="body">
      <metal:block define-slot="body">
      </metal:block>
    </div>

    <table id="portal-columns">
        <tbody>
            <tr>

                <!-- start of the left (by default at least) column -->
                <td id="portal-column-one"
                    metal:define-slot="column_one">
                    <metal:portlets define-slot="portlets_one">

                    </metal:portlets>
                </td>
                <!-- end of the left (by default at least) column -->

                <td id="portal-column-content">
                <div id="region-content">
                    <metal:main define-slot="main" tal:condition="nothing">
                        <p>Main content area.</p>
                    </metal:main>
                </div>
                </td>

                <!-- start of right (by default at least) column -->
                <td id="portal-column-two" metal:define-slot="column_two">
                        <metal:portlets define-slot="portlets_two">

                        </metal:portlets>
                </td>
                <!-- end of the right (by default at least) column -->

            </tr>
        </tbody>
    </table>
    <!-- end column wrapper -->

    <div class="visualClear">&nbsp;</div>

    <div class="footer">
        Copyright 2004-2005 Canonical Ltd. &ndash;  <a href="/legal">Legalese</a>
    </div>
</body>
</html>
</metal:page>
