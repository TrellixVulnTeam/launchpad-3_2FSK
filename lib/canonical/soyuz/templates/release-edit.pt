<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="rosetta">

<head>
    <!-- Plone style sheet -->
    <style type="text/css" media="screen">@import url(plone.css);</style>
    <!-- Rosetta style sheet -->
    <style type="text/css" media="screen">@import url(rosetta.css);</style>
</head>

<body>

<div metal:fill-slot="main">

   <h1>Editing <span tal:content="context/release/title">Release</span> Release 
	Information </h1>

    <form name="add" action="" method="POST">
	<table>

	<tr><th>Name</th> <th><input type="text" name="name" 
	tal:attributes="value context/release/name|request/release/name"/>
	</th></tr>

	<tr><th>Title</th> <th><input type="text" name="title" 
	tal:attributes="value context/release/title|request/release/title"/>
	</th></tr>

    	<tr><th> Description</th> <th><input type="text" name="description" 
	tal:attributes="value context/release/description|request/release/description"/></th></tr>

	<tr><th>Version</th> <th><input type="text" name="version" 
        tal:attributes="value context/release/version|request/release/version"/> </th> </tr>

	<tr><th></th><th><input type="submit" value="ok"/></th></tr>

	</table>
    </form>



</div>

</body>
</html>
