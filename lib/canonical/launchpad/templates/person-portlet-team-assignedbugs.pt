<div class="portlet" tal:condition="context/myactivememberships">

  <h2>Bugs assigned to</h2>

  <div class="portletBody portletContent">

    <ul tal:repeat="membership context/myactivememberships">
      <li>
        <a tal:attributes="href string:${membership/team/fmt:url}/+assignedbugs"
          ><span tal:replace="membership/team/browsername" /></a>
      </li>
    </ul>
    
  </div>

</div>
