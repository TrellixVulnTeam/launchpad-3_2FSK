<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  metal:use-macro="view/macro:page/main_only"
  i18n:domain="launchpad"
>
<body>

<div metal:fill-slot="main">
  <div class="top-portlet">
    <div metal:use-macro="context/@@launchpad_form/form" />
  </div>

  <div id="related-actions" class="portlet">
    <h2>Related actions</h2>

    <ul tal:define="menu context/menu:overview">
      <li>
        <a tal:replace="structure menu/edit/fmt:link" />
      </li>
      <li>
        <a tal:replace="structure menu/branding/fmt:link" />
      </li>
      <li>
        <a tal:replace="structure menu/review_license/fmt:link" />
      </li>
      <li>
        <a tal:replace="structure menu/administer/fmt:link" />
      </li>
    </ul>
  </div>
</div>

</body>
</html>

