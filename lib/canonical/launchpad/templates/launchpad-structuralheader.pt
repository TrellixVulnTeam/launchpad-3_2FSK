<div id="structuralobjectheading">
  <img alt="" src="/@@/nyet-heading" />
  <h1 class="major" tal:content="view/getMajorHeading">Ubuntu 6.10</h1>
  <div class="minor" tal:content="view/getMinorHeading">Edgy Eft</div>
</div>

      <div id="structuralobjectchildren">
        <tal:if condition="python:view.num_lists == 0">
          (no children)
        </tal:if>
        <tal:if condition="python:view.num_lists == 1">
          <span class="primary">
            <span tal:repeat="child view/children"
                  tal:replace="child" />
            <span tal:condition="view/more_children">more</span>
          </span>
        </tal:if>
        <tal:if condition="python:view.num_lists == 2">
          <span class="primary">
            <span tal:repeat="child view/children"
                  tal:replace="child" />
            <span tal:condition="view/more_children">more</span>
          </span>
          <span class="secondary">
            <span tal:repeat="child view/altchildren"
                  tal:replace="child" />
            <span tal:condition="view/more_altchildren">more</span>
          </span>
        </tal:if>

</div>
