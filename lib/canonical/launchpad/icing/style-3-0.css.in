/*

Use the following 'application' colors for table column headings,
app stats etc.

Launchpad-Gold: #F8C300
Answers: #3840BE
Blueprints: #3594BB
Bugs: #9F2B33
Code: #D18B39
Registry: default (i.e. black)
Translations: #BB3A84

*/

strong.answers-stat {
    color: #3840BE;
    }
strong.blueprints-stat {
    color: #3594BB;
    }
strong.bugs-stat {
    color: #9F2B33;
    }
strong.code-stat {
    color: #D18B39;
    }
strong.registry-stat {
    }
strong.translations-stat {
    color: #BB3A84;
    }
.launchpad-gold {
    color: #F8C300;
    }


/* Default text presentation */
html, body {
    font-family: bitstream vera sans, dejavu sans, verdana, sans-serif;
    font-size: 93%;
    }
body.private {
    /* It must be obvious to the user that the context is private */
    background: url("/@@/private-y-bg") top left repeat-y;
    }
h1 {
    clear: none;
    padding-top: 6px; /* An offset from the logo. */
    font-size: 197%;
    }
h2 {
    margin-bottom: 0.3em;
    margin-top: 0.3em;
    font-size: 123.1%;
    color: #5a5a5a;
    }
h3 {
    font-size: 108%;
    }
p {
    width: auto;
    margin-bottom: 0.8em;
    }
p, li, dt, dd, blockquote, .narrow {
    max-width: 45em; /* Wrap the text before the eye gets lost. */
    }
pre, code, samp, tt, .console {
  font-size: 116%;
  margin-bottom: 0.8em;
    }
ol {
       margin-left: 1.8em;
    }
ol li {
       list-style: decimal outside;
       }
li {
    padding-bottom: .3em;
    }
dt {
    margin-bottom: .3em;
    margin-top: 0.5em;
    }
dd {
    margin-bottom: .8em;
}
th {
    text-align: right;
    }
thead th, tr.thead th {
    text-align: center;
    vertical-align: bottom;
    }

form {
    margin-bottom: 1em;
}
form h1 {
    margin-bottom: 1em;
    }

form table th, form table td {
    padding: 2px;
    }
form table tbody th {
    font-weight: bold;
    }
input[type='image'] {
    vertical-align: middle;
    }
em {
    font-style: italic;
    }
strong {
    font-weight: bold;
    }

.unseen {display: none;}


/* Common presentations */
div.see-all {
    font-size: 123.1%; /* Same as a h2 */
    text-align:right;
    }
span.nolink {
    font-weight: bold;
    }
span.see-all {
