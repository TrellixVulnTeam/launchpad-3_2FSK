<metal:page define-macro="master"><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
  <metal:top_slot define-slot="top" />
  <html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    <head>
      <title tal:content="CONTEXTS/fmt:pagetitle">Launchpad</title>
      <link
        rel="stylesheet"
        href="/+icing/marketing-style.css"
        type="text/css"
        media="screen"
      />
      <!--[if IE]>
      <link
        rel="stylesheet"
        href="/+icing/marketing-winie.css"
        type="text/css"
        media="screen"
      />
      <![endif]-->
    </head>
    <body tal:define="vhosts modules/canonical.launchpad.webapp.vhosts/allvhosts/configs">
      <a id="top"></a>
      <div id="breadcrumb">
        <dl>
          <dt>Your location:</dt>
          <dd>
            <ul metal:define-slot="breadcrumbs">
              <li><a href="/">Home</a></li>
            </ul>
          </dd>
        </dl>
      </div> <!-- breadcrumb -->
      <div id="wrapper">
        <div id="header">
          <h1><a href="/" title="Launchpad">Launchpad</a></h1>
          <dl id="navigation">
            <dt>Navigation<dt>
            <dd>
              <ul>
                <li id="liHome"><a href="/+about"
                                   tal:attributes="href string:${vhosts/mainsite/rooturl}+about"
                                   >Overview</a></li>
                <li id="liAccount"
                    tal:condition="not: request/lp:person"
                    ><a href="+login"
                        tal:attributes="href string:${request/URL}/+login"
                        >Log in / Register</a></li>
                <li id="liTour"><a href="/+tour"
                                   tal:attributes="href string:${vhosts/mainsite/rooturl}+tour"
                                   >Take a tour</a></li>
              </ul>
            </dd>
          </dl>
          <metal:navigation define-slot="navigation" />
        </div> <!-- header -->
        <div id="main">
          <metal:contents define-slot="contents" />
        </div> <!-- main -->
        <div id="footer">
          <p class="left">Copyright&nbsp;2004-2007&nbsp;<a href="http://canonical.com/">Canonical&nbsp;Ltd.</a></p>
          <p class="right"> <a href="/legal">Terms&nbsp;of&nbsp;Use</a> |
        <a href="/feedback">Help&nbsp;improve&nbsp;Launchpad</a> |
        <a href="/faq">FAQ</a></p>
        </div> <!-- footer -->
      </div> <!-- wrapper -->
    </body>
  </html>
</metal:page>
