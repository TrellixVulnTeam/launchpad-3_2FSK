<div
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:lpm="http://ns.launchpad.net/lpm"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  >
  <label tal:condition="view/title|nothing"
         tal:content="string:${view/title}:">Menu title</label>
  <ul>
    <li tal:repeat="link view/links">
      <tal:link-content replace="structure link/@@+link">
        Link text
      </tal:link-content>
    </li>
  </ul>
</div>
