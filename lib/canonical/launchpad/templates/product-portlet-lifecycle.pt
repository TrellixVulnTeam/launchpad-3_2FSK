<div class="portlet">
  <h2>Lifecycle</h2>
  <div class="portletBody portletContent">
    <table>
      <tbody>
        <tr>
          <th>Registered:</th>
          <td tal:content="context/datecreated/fmt:date"></td>
        </tr>
        <tr>
          <th>Registrant:</th>
          <td>
            <a
              tal:attributes="href context/owner/fmt:url"
              tal:content="context/owner/browsername"
            >ownername</a>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
