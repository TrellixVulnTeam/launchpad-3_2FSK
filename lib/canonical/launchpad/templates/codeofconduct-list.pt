<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
    i18n:domain="launchpad"
>
  <body>
    <metal:heading fill-slot="pageheading">
      <h1>Available codes of conduct</h1>
    </metal:heading>

<div metal:fill-slot="main">

      <p>
        Select a code of conduct to sign or download it.
      </p>

      <ul class="document">
         <li tal:repeat="code context"> 
            <a tal:content="code/title" 
               tal:attributes="href string:${code/version}"/> 
         </li>
      </ul>

</div>

</body>
</html>
