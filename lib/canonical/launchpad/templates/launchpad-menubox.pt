<div
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  tal:condition="not:nocall:body | nothing"
  id="actions"
  class="portlet"
>
  <ul>
    <tal:block repeat="link view/contextmenuitems">
      <li tal:replace="structure link/@@+link">
        Link
      </li>
    </tal:block>
    <tal:block repeat="link view/applicationmenuitems">
      <li tal:replace="structure link/@@+link">
        Link
      </li>
    </tal:block>
  </ul>
</div>
