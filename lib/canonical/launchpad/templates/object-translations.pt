<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="context/@@main_template/master"
  i18n:domain="launchpad"
>

  <body>

    <metal:portlets fill-slot="portlets_one">
      <div tal:replace="structure context/@@+portlet-details" />
      <div tal:replace="structure context/@@+portlet-preflangs" />
    </metal:portlets>

    <metal:heading fill-slot="pageheading">
      <h1>
        Translation templates for
        <tal:title replace="context/title">
          evolution in Ubuntu Feisty
        </tal:title>
      </h1>
    </metal:heading>

    <div metal:fill-slot="main">
      <div tal:replace="structure context/@@+potlist" />
    </div>

  </body>
</html>
