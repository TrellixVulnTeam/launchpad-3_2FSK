<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="view/macro:page/onecolumn"
  i18n:domain="launchpad"
>
<body>

  <metal:heading fill-slot="before_nav_menu"
                 tal:content="structure context/branch_merge_proposal/source_branch/@@+heading"/>

  <div metal:fill-slot="main">

    <tal:proposal-heading tal:replace="structure context/branch_merge_proposal/@@+heading"/>

    <h1>Code review comment</h1>
    <tal:content replace="structure context/@@+fragment"/>
    <p>&laquo;&nbsp;<a href="#" tal:attributes="href context/branch_merge_proposal/fmt:url">Back to merge
    proposal</a></p>
  </div>
</body>
</html>
