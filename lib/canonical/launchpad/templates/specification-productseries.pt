<tal:tag condition="view/update"/>
<html
    xmlns="http://www.w3.org/1999/xhtml"
    xml:lang="en"
    lang="en"
    metal:use-macro="context/@@main_template/master"
    i18n:domain="launchpad">

<body>

<metal:leftportlets fill-slot="portlets_one">
  <div tal:replace="structure context/@@+portlet-details" />
</metal:leftportlets>

<metal:rightportlets fill-slot="portlets_two">
  <div tal:replace="structure context/@@+portlet-actions" />
</metal:rightportlets>

  <div metal:fill-slot="main">

    <div metal:use-macro="context/@@launchpad_editform/editform">

    <div metal:fill-slot="extra_info" class="documentDescription">
      Use this form to target this specification for implementation in a
      specific release series of
      <span tal:replace="context/product/displayname">Firefox</span>.
    </div>

  </div>
    
</div>
</body>
</html>
