<!--  packages stuff -->

  <page for="canonical.launchpad.interfaces.IPackages"
        permission="zope.Public"
        name="index.html"
        template="../launchpad/templates/packages-index.pt"
        lp:url="Soyuz/packages/"
        />

  <page for="canonical.launchpad.interfaces.IPackageSet"
        permission="zope.Public"
        name="index.html"
        template="../launchpad/templates/package-list.pt"
        lp:url="Soyuz/distros/$Distribution.name/$Release.name/packages/$package_type/$Package.id/index.html"
        />

  <page for="canonical.launchpad.interfaces.IPackageSet"
        permission="zope.Public"
