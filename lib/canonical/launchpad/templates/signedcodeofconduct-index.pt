<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="rosetta">
<body>

<metal:rightportlets fill-slot="portlets_two">
        <div tal:replace="structure context/@@+portlet-actions" />
</metal:rightportlets>


<div metal:fill-slot="main">

      <h1>Signed Code of Conduct Entry</h1>

      <div class="documentDescription">
        Signed Code of Conduct.
      </div>

      <p><dfn>Displayname:</dfn> 
         <span tal:replace="context/displayname">CoC</span></p>


      <p><dfn>Status:</dfn> 
       <span tal:omit-tag="" tal:condition="context/active">Active</span>
       <span tal:omit-tag="" tal:condition="not:context/active">Inactive</span>
      </p>

      <p><dfn>DateCreated:</dfn> 
         <span tal:replace="context/datecreated/fmt:datetime">DATE</span></p>

      <tal:block condition="context/recipient">
         <p><dfn>Recipient:</dfn> 
         <span tal:replace="context/recipient/browsername">RECIPIENT</span></p>
      </tal:block>

      <tal:block condition="context/admincomment">
          <p><dfn>AdminComment:</dfn></p>
          <pre tal:content="context/admincomment">COMMENT</pre>
      </tal:block>                  

      <tal:block condition="context/signedcode">
          <p><dfn>SignedCode:</dfn></p>
          <pre tal:content="context/signedcode">SIGNEDCODE</pre>
      </tal:block>                  

</div>

</body>
</html>
