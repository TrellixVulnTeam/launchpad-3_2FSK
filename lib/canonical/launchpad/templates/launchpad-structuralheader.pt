    <div id="structuralobject">
      <img alt="" src="/@@/nyet-heading" />
      <div class="secondary"><h2 tal:content="view/getMinorHeading">structural object secondary</h2></div>
      <div class="primary" tal:content="view/getMajorHeading">structural object primary</div>
      <p id="structuralobjectchildren">
        <tal:if condition="python:view.num_lists == 0">
          (no children)
        </tal:if>
        <tal:if condition="python:view.num_lists == 1">
          <span class="primary">
            <span tal:repeat="child view/children"
                  tal:replace="child" />
            <span tal:condition="view/more_children">more</span>
          </span>
        </tal:if>
        <tal:if condition="python:view.num_lists == 2">
          <span class="primary">
            <span tal:repeat="child view/children"
                  tal:replace="child" />
            <span tal:condition="view/more_children">more</span>
          </span>
          <span class="secondary">
            <span tal:repeat="child view/altchildren"
                  tal:replace="child" />
            <span tal:condition="view/more_altchildren">more</span>
          </span>
        </tal:if>

      </p>
    </div>
