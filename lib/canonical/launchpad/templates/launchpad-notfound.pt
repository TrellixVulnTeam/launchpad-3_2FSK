<tal:layout define="columns1 default">
<html metal:use-macro="context/@@main_template/master">
  <body>
    <metal:heading fill-slot="pageheading">
      <h1 i18n:translate="">Page not found</h1>
    </metal:heading>
    <div metal:fill-slot="main">
      <p i18n:translate="">
        There&#8217;s no page with this address in Launchpad.
      </p>
      <p i18n:translate="">
        If you got here from a link elsewhere on Launchpad,
        sorry about that.
        We&#8217;re working hard to fix those broken links.
      </p>
      <p i18n:translate="">
        Otherwise, check that you typed the address correctly.
      </p>
      <p tal:condition="request/oopsid">
        The reference for this error is
        <tt class="oopsid" tal:content="request/oopsid">OOPS-A1234</tt>.
        Please include it in any related bug report or email.
      </p>
    <tal:replace tal:replace="structure view/maybeShowTraceback" />
    </div>
  </body>
</html>
</tal:layout>
