<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="launchpad">

<head>
    <title metal:fill-slot="title">Translations Made By
      <span tal:replace="context/displayname">Name</span></title>
    <!-- Plone style sheet -->
    <style type="text/css" media="screen">@import url(plone.css);</style>
    <!-- Soyuz style sheet -->
    <style type="text/css" media="screen">@import url(soyuz.css);</style>
</head>

<body>

<metal:leftportlets fill-slot="portlets_one">
<div tal:replace="structure view/leftMenu" />
</metal:leftportlets>

<div metal:fill-slot="main">

<div id="content" class="documentEditable">

    <ul class="contentViews">
      <li id="contentview-view" class="plain">
        <a href=".">Profile</a>
      </li>

      <!--
      <li tal:condition="context/packages" id="contentview-packages"
      -->
      <li id="contentview-packages" class="plain">
        <a href="+packages">Packages</a>
      </li>

      <!--
      <li tal:condition="context/bugs" id="contentview-bugs" class="plain">
      -->
      <li id="contentview-bugs" class="plain">
        <a href="+bugs">Bugs</a>
      </li>

      <!--
      <li tal:condition="context/translations" id="contentview-translations"
      -->
      <li id="contentview-translations" class="selected">
        <a href="+translations">Translations</a>
      </li>

      <!--
      <li tal:condition="context/activities" id="contentview-karma"
      -->
      <li id="contentview-karma" class="plain">
        <a href="+karma">Karma</a>
      </li>
    </ul>
    <div class="documentContent" id="region-content">

      <h1>Translations Made by
      <span tal:content="context/displayname">context</span></h1>

</div>
</div>
</div>


</body>
</html>
