<table class="listing">
  <tr>
    <th> Binary Package </th>
  </tr>
  <tr tal:repeat="binpackageapp context">
    <td tal:define="binpackage binpackageapp/binarypackage"> 
      <a tal:content="binpackage/shortdesc" tal:attributes="href binpackage/name">TITLE</a> 
    </td>
  </tr>
</table>



<!-- </div> -->
</div>

