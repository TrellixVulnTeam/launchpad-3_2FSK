<div
  class="navigation"
  tal:define="prev_page_url context/prevBatchURL;
              next_page_url context/nextBatchURL">
  <a tal:condition="prev_page_url"
     tal:attributes="href prev_page_url"
     rel="previous">
     &laquo; previous
  </a>&nbsp;
  <span tal:omit-tag=""
        tal:repeat="page_url context/batchPageURLs">
    <a href="."
       tal:content="python:page_url.keys()[0]"
       tal:condition="python:len(page_url) > 1"
       tal:attributes="href python:page_url.values()[0]">[1]</a>
  </span>
  &nbsp;<a tal:condition="next_page_url"
     tal:attributes="href next_page_url"
     rel="next">
     next &raquo;
  </a>
  <span tal:condition="not: next_page_url" tal:omit-tag>
    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
  </span>
</div>
<tal:block tal:define="total context/batch/total">
  <strong tal:content="context/batch/startNumber">1</strong>
  &rarr;
  <strong tal:content="context/batch/endNumber">10</strong>
  of
  <span tal:replace="total">42</span> result<span
      tal:omit-tag="" tal:condition="python: total != 1">s</span>
</tal:block>
