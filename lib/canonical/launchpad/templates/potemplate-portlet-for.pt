
<div class="portlet">

  <h2>Origin</h2>

  <div class="portletBody">
    <div class="portletContent">
    <tal:block condition="context/distroseries">
      From source package:
      <b>Distribution:</b>
      <span tal:replace="context/distroseries/distribution/displayname">
        Ubuntu
      </span><br />
      <b>Series:</b>
      <span tal:replace="context/distroseries/name">
        Hoary
      </span><br />
      <b>Source:</b>
      <span tal:replace="context/sourcepackagename/name">
        Apache
      </span><br />
    </tal:block>
    <tal:block condition="context/productseries">
      From <b>upstream</b> project branch:
      <br />
      <tal:block condition="context/productseries/product/project">
        <b>Project:</b>
        <span tal:replace="context/productseries/product/project/displayname">
          Apache
        </span><br />
      </tal:block>
      <b>Product:</b>
      <span tal:replace="context/productseries/product/displayname">
        Apache Web Server
      </span><br />
      <b>Branch:</b>
      <span tal:replace="context/productseries/displayname">
        MAIN
      </span><br />
    </tal:block>
    </div>
  </div>
</div>

