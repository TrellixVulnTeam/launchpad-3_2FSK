	  lp:url="Soyuz/distros/$Distribution.name/team/$Release.name/index.html"
	/>
   </pages>

<traverse for="canonical.soyuz.sql.PeopleApp" getter="__getitem__"/>

   <pages for="canonical.soyuz.sql.PeopleApp"
	  allowed_attributes="results"
	  permission="zope.Public">

	<page  name="+list"
          template="../launchpad/templates/people-list.pt"
	  lp:url="Soyuz/people/+list"
	/>
   </pages>

  <page for="canonical.soyuz.sql.PeopleApp"
         permission="zope.Public"
	 class="canonical.soyuz.browser.PeopleSearchView"
	 template="../launchpad/templates/soyuz-people-index.pt"
         lp:url="Soyuz/people/index.html"
	 name="index.html"
       />

  <page for="canonical.soyuz.sql.PeopleApp"
         permission="zope.Public"
	 class="canonical.soyuz.browser.PeopleAddView"
	 template="../launchpad/templates/people-add.pt"
         lp:url="Soyuz/people/+add"
	 name="+add"
       />

   <pages for="canonical.soyuz.sql.PersonApp"
	  allowed_attributes="results context"
	  permission="zope.Public">

	<page  name="index.html"
          template="../launchpad/templates/person-index.pt"
	  lp:url="Soyuz/people/$person.id/index.html"
	/>

	<page  name="+key"
          template="../launchpad/templates/person-key.pt"
	  lp:url="Soyuz/people/$person.id/+key"
	/>
   </pages>

  <page for="canonical.soyuz.sql.PersonApp"
         permission="zope.Public"
	 class="canonical.soyuz.browser.TeamAddView"
	 template="../launchpad/templates/soyuz-person-add.pt"
         lp:url="Soyuz/people/$person.id/+add"
	 name="+add"
       />

  <page for="canonical.soyuz.sql.PersonApp"
         permission="zope.Public"
	 class="canonical.soyuz.browser.PersonEditView"
	 template="../launchpad/templates/person-edit.pt"
         lp:url="Soyuz/people/$person.id/+edit"
	 name="+edit"
       />

  <page for="canonical.soyuz.sql.PersonApp"
