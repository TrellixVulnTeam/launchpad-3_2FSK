<metal:page
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  define-macro="master"><metal:doctype define-slot="doctype"><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"></metal:doctype>
<metal:block define-slot="top_slot" />
<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  tal:attributes="lang view/lang|default_language|default;
                  xml:lang view/lang|default_language|default;
                  dir view/dir|string:ltr">

  <head>
    <title tal:content="CONTEXTS/fmt:pagetitle">Launchpad</title>

    <style type="text/css" media="all"
      tal:content="string:@import url(${request/icing_url}/style.css);" />
    <style type="text/css" media="print"
      tal:content="string:@import url(${request/icing_url}/print.css);" />
    <style type="text/css" media="all"
      tal:content="string:@import url(${request/icing_url}/lp-extra.css);" />
    <tal:XXX condition="nothing">
      # XXX: salgado 20080415 We cannot use tal inside the comment below
      # (which is only interpreted by IE), so we have to use
      # /+icing-ubuntu/ie.css rather than using request/icing_url.
    </tal:XXX>
		<!--[if lt IE 7]>
    <style type="text/css" media="all">
      @import url(/+icing-ubuntu/ie.css);</style>
		<![endif]-->

    <metal:block define-slot="head_epilogue"></metal:block>

  </head>

  <body class="sidebars">

    <div id="round" class="roundme">
      <img id="topcap" alt=""
        tal:attributes="src string:${request/icing_url}/cap-top.png" />
    <div id="layout" class="container clear-block">
    <div id="header">
      <div id="logo-floater">
        <h1><a href="http://www.ubuntu.com" title="Ubuntu">
            <img tal:attributes="src string:${request/icing_url}/logo.png" alt="Ubuntu" id="logo" /><span>Ubuntu</span></a></h1>
      </div>
      <ul class="links primary-links"><li class="first menu-1-1-2"><a href="http://www.ubuntu.com/products" title="Learn about the Ubuntu products" class="menu-1-1-2">Products</a></li>
      <li class="menu-1-2-2"><a href="http://www.ubuntu.com/support" title="Get help with Ubuntu" class="menu-1-2-2">Support</a></li>
      <li class="menu-1-3-2"><a href="http://www.ubuntu.com/community" title="Learn about and participate in the Ubuntu community" class="menu-1-3-2">Community</a></li>
      <li class="menu-1-4-2"><a href="http://www.ubuntu.com/partners" title="Find Ubuntu related products and services" class="menu-1-4-2">Partners</a></li>
      <li class="last menu-1-5-2"><a href="http://www.ubuntu.com/news" title="Find out what&#039;s new with Ubuntu" class="menu-1-5-2">News</a></li>
      </ul>

      <div id="masthead" class="clear-block column span-24">
        <div id="block-block-7" class="clear-block block block-block">
          <div class="content"><div class="short-banner">
          <img tal:attributes="src string:${request/icing_url}/masthead-shipit.png" width="861" height="100" border="0" usemap="#map" />
          <map name="map"><area shape="rect" coords="655,1,855,96" alt="Visit the Ubuntu Store - Buy Ubuntu branded merchandise, DVDs and world class support" href="https://shop.canonical.com" /></map>
          </div></div>
        </div>
      </div>
    </div> <!-- /header -->

        <div id="leftsidebar" class="sidebar column span-7">
        <div class="block block-menu" id="block-menu-53">
        <div class="content">
          <ul class="menu">
          <li class="collapsed"><a href="http://www.ubuntu.com/products/whatisubuntu">What is Ubuntu?</a></li>

          <li class="expanded"><a href="http://www.ubuntu.com/getubuntu">Get Ubuntu</a>
          <ul class="menu">
            <li class="leaf"><a href="http://www.ubuntu.com/getubuntu/download">Download</a></li>
            <li class="collapsed"><a href="http://www.ubuntu.com/getubuntu/purchase">Purchase</a></li>
            <li class="collapsed"><a class="active">Request a CD</a></li>
            <li class="leaf"><a href="http://www.ubuntu.com/getubuntu/releasenotes">Release Notes</a></li>
            <li class="leaf"><a href="http://www.ubuntu.com/getubuntu/countdown">Countdown</a></li>
            <li class="leaf"><a href="http://www.ubuntu.com/getubuntu/mirror">Mirror Ubuntu</a></li>
          </ul>
          </li>
          <li class="leaf"><a href="http://www.ubuntu.com/products/softwarecatalogue">Software Catalogue</a></li>
          <li class="leaf"><a href="http://www.ubuntu.com/products/merchandise">Merchandise</a></li>
          <li class="collapsed"><a href="http://www.ubuntu.com/products/casestudies">Case Studies</a></li>
          </ul>
        </div>
        </div>
        </div>

      <div id="maincontent" class="column span-16 append-1 last">
        <div class="breadcrumb">
          <span class="right push-1">
            <tal:block condition="request/lp:person">
              <form action="/+logout" class="login" method="post">
                Logged in as <span tal:replace="request/lp:person/displayname" />
                  &ndash;
                <input type="submit" value="Log Out" />
              </form>
            </tal:block>

            <tal:block condition="not: request/lp:person">
              Not logged in &#8212;
              <a href="/login">
                Log In / Register
              </a>
            </tal:block>
          </span>
          <a href="http://www.ubuntu.com">Home</a>
          &#8250; <a href="http://www.ubuntu.com/getubuntu">Get Ubuntu</a>
          &#8250; <strong>Request a Free CD</strong>
        </div>

        <tal:maintenance replace="structure context/@@+maintenancemessage" />
        <tal:notifications define="notifications request/notifications"
                           condition="notifications">
          <metal:notifications
            use-macro="context/@@+main-template-macros/notifications" />
        </tal:notifications>

        <metal:block define-slot="body">
          PAGE CONTENT HERE
        </metal:block>

        <span class="clear"></span>
      </div> <!-- /.left-corner, /.right-corner, /#squeeze, /#center -->

      <div id="footer" class="column span-24">&copy; 2008 Canonical Ltd.</div>

    </div> <!-- /layout -->
    <img id="bottomcap" alt=""
         tal:attributes="src string:${request/icing_url}/cap-bottom.png" />
  </div>

  </body>
</html>
</metal:page>
