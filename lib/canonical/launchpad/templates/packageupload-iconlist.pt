<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  omit-tag="">

<div tal:attributes="id string:queue${context/id}-iconlist">
  <img tal:condition="context/contains_source"
       alt="[Source]" src="/@@/package-source" title="Source"/>
  <img tal:condition="context/contains_build"
       alt="[Build]" src="/@@/package-binary" title="Binary"/>
  <img tal:condition="context/contains_translation"
       alt="[Translation]" src="/@@/translation-file"
       title="Translation"/>
  <img tal:condition="context/contains_installer"
       alt="[Installer]" src="/@@/ubuntu-icon"
       title="Installer"/>
  <img tal:condition="context/contains_upgrader"
       alt="[Upgrader]" src="/@@/ubuntu-icon"
       title="Upgrader"/>
  <img tal:condition="context/contains_ddtp"
       alt="[Debian Description Translation Project Indexes]"
       src="/@@/ubuntu-icon"
       title="Debian Description Translation Project Indexes"/>
  <a tal:attributes="href context/changesfile/http_url;
                     title string:Changes file for ${context/displayname}">
    <tal:name replace="string: ${context/displayname}"/>
  </a>
  <tal:version replace="
        string: (${context/displayarchs})"/>
</div>
</tal:root>
