<tal:tag condition="view/update"/>
<html
    xmlns="http://www.w3.org/1999/xhtml"
    xml:lang="en"
    lang="en"
    metal:use-macro="context/@@main_template/master"
    i18n:domain="launchpad">

<body>

<div metal:fill-slot="main">

      <div>
        <img alt="" src="/@@/blueprint" />
        <a tal:attributes="href context/fmt:url" tal:content="context/title" />
      </div>

  <div metal:use-macro="context/@@launchpad_editform/editform">

  <div metal:fill-slot="extra_info" class="documentDescription">
    Select the milestone of
    <span tal:replace="context/target/displayname">Ubuntu</span>
    in which you would like this feature to be implemented.
  </div>

</div>
    
</div>
</body>
</html>
