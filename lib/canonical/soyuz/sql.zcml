     set_attributes="name title description version"
     />
  </content>
  

  <content class="canonical.soyuz.sql.Release">
    <allow interface="canonical.launchpad.interfaces.IRelease"/>
    <require
     permission="zope.Public"
     set_attributes="distribution name title description version components sections releasestate datereleased owner"
     />
  </content>

