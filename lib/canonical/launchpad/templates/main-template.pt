<metal:page define-macro="master">
<metal:doctype define-slot="doctype">
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
</metal:doctype>
<metal:block define-slot="top_slot" />
<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      lang="en"
      tal:attributes="lang default_language|default;
                      xml:lang default_language|default;">
  <head>
    <metal:block define-slot="title"></metal:block>
    <!-- Plone style sheet -->
    <style type="text/css" media="screen">@import url(/++resource++plone.css);</style>
    <!-- Launchpad style sheet -->
    <style type="text/css" media="screen">@import url(/++resource++launchpad.css);</style>
    <!-- Launchpad JS code -->
    <script type="text/javascript" src="/++resource++launchpad.js"></script>
    <link rel="shortcut icon" href="/++resource++defaultFavicon.gif" />
  </head>

  <body>
      <div id="portal-top">
               <a href="/">
                  <img src="/++resource++launchpad.png" alt="Launchpad Home" />
               </a>
               <span tal:content="context/title | string: No Context Title">
                The Context Object Title
               </span><br />
      </div>

          <ul id="portal-globalnav"
              tal:define="path_info request/PATH_INFO">

            <li class="plain">
              <a href=".">Home</a>
            </li>

            <li class="selected"
                tal:condition="python:'/people' in path_info">
              <a href="/people/">People</a>
            </li>
            <li class="plain"
                tal:condition="python:'/people' not in path_info">
              <a href="/people/">People</a>
            </li>

            <li class="selected"
                tal:condition="python:'/products' in path_info">
              <a href="/products">Products</a>
            </li>
            <li class="plain"
                tal:condition="python:'/products' not in path_info">
              <a href="/products">Products</a>
            </li>

            <li class="selected"
                tal:condition="python:'/distros' in path_info">
              <a href="/distros">Distributions</a>
            </li>
            <li class="plain"
                tal:condition="python:'/distros' not in path_info">
              <a href="/distros">Distributions</a>
            </li>

            <li class="selected"
                tal:condition="python:'/projects' in path_info">
              <a href="/projects">Projects</a>
            </li>
            <li class="plain"
                tal:condition="python:'/projects' not in path_info">
              <a href="/projects">Projects</a>
            </li>

            <li class="selected"
                tal:condition="python:'/calendar' in path_info">
              <a href="/calendar">Calendar</a>
            </li>
            <li class="plain"
                tal:condition="python:'/calendar' not in path_info">
              <a href="/calendar">Calendar</a>
            </li>

          </ul>

      <div id="portal-personaltools"
           tal:define="path_info request/PATH_INFO">

        <span tal:condition="request/lp:person">
            Logged in as
            <a tal:attributes="href string:/people/${request/lp:person/name}">
            <span tal:replace="request/lp:person/displayname">Foo Bar</span></a>
            &mdash;
            <a href="/+logout">Log Out</a>
        </span>

        <span tal:condition="not: request/lp:person" tal:omit-tag>
            <a href="/+login">Log In</a>
            &mdash;
            <a href="/+register">Join</a>
        </span>

        <span tal:condition="python:'/rosetta' in path_info"
              tal:omit-tag>
            &mdash;
            <a href="/rosetta/translator">Translation Dashboard</a>
            &mdash;
            <a href="/rosetta/prefs">Rosetta Prefs</a>
        </span>

      </div>

      <div id="portal-breadcrumbs">
        You are here:
        <span tal:replace="structure request/fmt:breadcrumbs" />
      </div>

      <div class="visualClear"></div>

      <table id="portal-columns">
        <tbody>
          <tr>

            <!-- start of the left (by default at least) column -->
            <td id="portal-column-one"
                metal:define-slot="column_one">
              <div class="visualPadding">
                <metal:portlets define-slot="portlets_one">

                </metal:portlets>
                &nbsp;
              </div>
            </td>
            <!-- end of the left (by default at least) column -->

            <td id="portal-column-content">
              <div id="region-content">
                  <metal:main define-slot="main">
                  <p>Main content area.</p>
                  </metal:main>
              </div>
            </td>

            <!-- start of right (by default at least) column -->
            <td id="portal-column-two"
                metal:define-slot="column_two">
              <div class="visualPadding">
                <metal:portlets define-slot="portlets_two">

                </metal:portlets>
                &nbsp;
              </div>
            </td>
            <!-- end of the right (by default at least) column -->

          </tr>
        </tbody>
      </table>
      <!-- end column wrapper -->

      <div class="visualClear">&nbsp;</div>

      <div id="portal-footer">
          Copyright 2004 Canonical Ltd.<br />
          <a href="/legal">Legal</a>
      </div>
  </body>
</html>
</metal:page>
