<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  xmlns:lp="http://launchpad.canonical.com"
  xmlns:i18n="http://namespaces.zope.org/i18n"
  i18n_domain="launchpad">

  <!-- DistroArchReleaseBinaryPackageRelease -->
  <content
    class="canonical.launchpad.database.DistroArchReleaseBinaryPackageRelease">
    <allow
      interface="canonical.launchpad.interfaces.IDistroArchReleaseBinaryPackageRelease" />
  </content>

  <browser:defaultView
    for="canonical.launchpad.interfaces.IDistroArchReleaseBinaryPackageRelease"
    name="+index" />

  <browser:url
    for="canonical.launchpad.interfaces.IDistroArchReleaseBinaryPackageRelease"
    path_expression="string:${name}/${version}"
    attribute_to_parent="distroarchrelease"
    />

  <browser:navigation
    module="canonical.launchpad.browser"
    classes="DistroArchReleaseBinaryPackageReleaseNavigation"
    />

  <browser:menus
    module="canonical.launchpad.browser.distroarchreleasebinarypackagerelease"
    classes="DistroArchReleaseBinaryPackageReleaseFacets"
    />

  <browser:pages
    for="canonical.launchpad.interfaces.IDistroArchReleaseBinaryPackageRelease"
    class="canonical.launchpad.browser.DistroArchReleaseBinaryPackageReleaseView"
    permission="zope.Public"
    >
    <browser:page
      name="+index"
      facet="overview"
      template="../templates/distroarchreleasebinarypackagerelease-index.pt"
      />

    <!-- DistroArchReleaseBinaryPackageRelease Portlets -->
    <browser:page
      name="+portlet-details"
      facet="overview"
      template="../templates/distroarchreleasebinarypackagerelease-portlet-details.pt"
      />
  </browser:pages>

</configure>
