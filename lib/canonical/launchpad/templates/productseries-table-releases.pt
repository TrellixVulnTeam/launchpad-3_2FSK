
<table class="listing "
  tal:condition="context/releases"
  tal:attributes="id string:series_${context/name}">
  <tbody class="lesser">
    <tr tal:repeat="release context/releases">
      <td><a tal:content="release/version"
             tal:attributes="href release/fmt:url">version</a></td> 
      <td tal:content="release/codename">code name</td>
      <td tal:content="release/datereleased/fmt:date">date</td>
    </tr>
  </tbody>
</table>

