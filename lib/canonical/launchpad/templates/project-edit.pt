<tal:tag condition="view/update"/>
<html
    xmlns="http://www.w3.org/1999/xhtml"
    xml:lang="en"
    lang="en"
    metal:use-macro="context/@@malone_template/master"
    i18n:domain="launchpad">

  <body>

    <div metal:fill-slot="main">

      <div metal:use-macro="context/@@launchpad_editform/editform">

          <h1 tal:condition="view/label"
              tal:content="view/label"
              metal:fill-slot="heading"
              >Form Label will come from ZCML label="xxx"</h1>

          <div metal:fill-slot="extra_info"
            class="documentDescription">
            Edit the Project Details using the form below. Your changes
            become visible immediately so please do take care when editing a
            field, particularly the display name and title fields.
          </div>

          <legend metal:fill-slot="editform_legend">
             Project Details
          </legend>
        
      </div>
    
    </div>

  </body>
</html>
