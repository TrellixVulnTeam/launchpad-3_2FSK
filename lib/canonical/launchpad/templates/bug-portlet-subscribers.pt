<div
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  class="portlet"
  id="portlet-subscribers"
  metal:define-macro="custom"
>
  <div class="section" tal:define="context_menu context/menu:context"
       metal:define-slot="heading">
    <div tal:content="structure context_menu/subscription/render" />
    <div tal:content="structure context_menu/addsubscriber/render" />
  </div>
  <script type="text/javascript"
          tal:define="revno modules/canonical.launchpad.versioninfo/revno | string:unknown;
                      rooturl modules/canonical.launchpad.webapp.vhosts/allvhosts/configs/mainsite/rooturl;
                      icingroot string:${rooturl}+icing/rev${revno}"
          tal:attributes="src string:${icingroot}/build/malone/bug-portlet-subscribers.js">
  </script>
</div>
