  <tr class="ppa_package_row">
     <td><a href="#"
            tal:attributes="onClick string:return toggleExpandableTableRow('pub${context/id}');"><img 
                src="/@@/treeCollapsed" alt=""
                tal:attributes="id string:pub${context/id}-arrow" /></a><span 
        tal:content="context/sourcepackagerelease/title">foo - 1.0</span></td>
     <td tal:condition="context/archive/owner/isTeam">
         <a id="uploader"
            tal:attributes="href context/sourcepackagerelease/dscsigningkey/owner/fmt:url"
            tal:content="context/sourcepackagerelease/dscsigningkey/owner/name"></a></td>
     <td tal:attributes="title context/datepublished/fmt:datetime"
         tal:content="context/datepublished/fmt:approximatedate"
         >2005-09-16</td>
     <td tal:content="context/status/title"
         >PUBLISHED</td>
     <td tal:content="context/sourcepackagerelease/uploaddistroseries/name"
         >dapper</td>
     <td tal:content="context/section/name">base</td>
     <td>
     </td>
  </tr>
  <tr tal:attributes="id string:pub${context/id}" style="display: none">
    <td colspan="5">
      <i tal:condition="not: view/published_source_and_binary_files">No files yet published</i>
      <ul tal:repeat="file view/published_source_and_binary_files"
          style="margin-top: 0; margin-bottom: 0">
        <li><a tal:attributes ="href file/archive_url"
               tal:content="string:${file/filename}"></a>
                (<span tal:content="file/filesize/fmt:bytes" />)</li>
      </ul>
    </td>
  </tr>
