<traverse for="canonical.soyuz.sql.DistroTeamApp" getter="__getitem__" />

   <pages for="canonical.soyuz.sql.DistroTeamApp"
	  allowed_attributes="results"
	  permission="zope.Public">

	<page  name="index.html"
          template="../launchpad/templates/team-index.pt"
	  lp:url="Soyuz/distros/$Distribution.name/team/index.html"
	/>
