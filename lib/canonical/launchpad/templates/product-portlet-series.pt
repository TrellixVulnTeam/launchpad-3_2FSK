<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  omit-tag="">

<div class="portlet" id="portlet-series">
  <h2>
    <tal:name content="context/displayname">Firefox</tal:name>
    release series
  </h2>

  <div class="portletBody portletContent">

  <ul>
    <li tal:repeat="series context/serieses"
        class="product">
      <a tal:attributes="href series/fmt:url"
         tal:content="series/displayname">Evolution MAIN</a>
    </li>
  </ul>
	
  </div>

</div>
</tal:root>
