            interface="lp.soyuz.interfaces.queue.IPackageUpload"/>
        <allow
            attributes="
                status"/>
        <require
            permission="zope.Public"
            attributes="
                id
                distroseries
                pocket
                changesfile
                signing_key
                archive
                sources
                builds
                customfiles
                custom_file_urls
                date_created
                sourcepackagerelease
                contains_source
                contains_build
                contains_translation
                contains_installer
                contains_upgrader
                contains_ddtp
                displayname
                displayarchs
                displayversion
                is_delayed_copy
                isPPA
                components"/>
