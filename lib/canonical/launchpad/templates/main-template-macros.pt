<macros
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="launchpad"
  tal:omit-tag=""
>
  <metal:notifications define-macro="notifications">
    <metal:base-notifications
      use-macro="context/@@+base-layout-macros/notifications" />
  </metal:notifications>

  <metal:load-javascript define-macro="load-javascript">
    <metal:base-load-javascript
      use-macro="context/@@+base-layout-macros/load-javascript" />
  </metal:load-javascript>

  <metal:launchpad-stylesheet define-macro="launchpad-stylesheet">
    <metal:base-launchpad-stylesheet
      use-macro="context/@@+base-layout-macros/launchpad-stylesheet" />
  </metal:launchpad-stylesheet>

  <metal:lp-client-cache define-macro="lp-client-cache">
    <metal:base-lp-client-cache
      use-macro="context/@@+base-layout-macros/lp-client-cache" />
  </metal:lp-client-cache>
</macros>
