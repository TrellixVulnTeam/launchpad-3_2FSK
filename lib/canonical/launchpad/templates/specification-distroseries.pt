<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  omit-tag="">

<tal:tag condition="view/update"/>
<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="context/@@main_template/master"
  i18n:domain="launchpad"
>

<body>

  <div metal:fill-slot="main">

      <div>
        <tal:icon replace="structure context/image:icon" />
        <a tal:attributes="href context/fmt:url" tal:content="context/title"
        >Blueprint title</a>:
      </div>

    <div metal:use-macro="context/@@launchpad_generalform/generalform">

    <p metal:fill-slot="extra_info">
      Select the release of
      <span tal:replace="context/distribution/displayname">Ubuntu</span>
      for which this blueprint should be a feature goal.
    </p>

  </div>

</div>
</body>
</html>
</tal:root>
