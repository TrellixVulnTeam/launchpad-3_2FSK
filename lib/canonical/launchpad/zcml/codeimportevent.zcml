<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  xmlns:i18n="http://namespaces.zope.org/i18n"
  xmlns:zope="http://namespaces.zope.org/zope"
  i18n_domain="launchpad">

  <!-- CodeImportEvent -->

  <class class="canonical.launchpad.database.CodeImportEvent">
    <allow interface="canonical.launchpad.interfaces.ICodeImportEvent" />
  </class>

  <!-- CodeImportEventSet -->

  <securedutility
    class="canonical.launchpad.database.CodeImportEventSet"
    provides="canonical.launchpad.interfaces.ICodeImportEventSet" >
    <allow interface="canonical.launchpad.interfaces.ICodeImportEventSet" />
  </securedutility>

  <!-- CodeImportEventToken -->

  <class class="canonical.launchpad.database.CodeImportEventToken">
    <allow interface="canonical.launchpad.interfaces.ICodeImportEventToken" />
  </class>

</configure>
