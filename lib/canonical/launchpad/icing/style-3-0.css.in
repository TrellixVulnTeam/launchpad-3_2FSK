/* Page layout */
.yui-d0 {
    margin: 10px 20px;
    }
.yui-t4 .yui-b {
    width: 21%;
    }
.yui-t4 .yui-main {
    clear: both;
    float: left;
    margin-right: -25%;
    }
.yui-t4 .yui-main .yui-b {
    width: auto;
    margin-right: 25%;
    }
.footer {
    clear: both;
    margin-top: 4em;
    padding-top: 0.5em;
    }
.footer .lp-arcana {
    background: url(/@@/footer-background.png) top left repeat-x;
    padding: 0.8em;
    -moz-border-radius: 3px 3px 0 0;
    -webkit-border-radius: 3px 3px 0 0;
    -khtml-border-radius: 3px 3px 0 0;
    border-radius: 3px 3px 0 0;
    height:2em;
    }
.footer .lp-arcana img {
    vertical-align: middle;
    }
.footer .sitemessage {
    text-align: right;
}

.footer .colophon {
    margin: 3em 3em 1em 3em;
    text-align: center;
    }
.footer .lp-branding {
    float: left;
    }
.footer form {
    display: inline;
    }
.portlet-border {
    border-top: 1px solid #EBEBEB;
    padding: 1em 0;
    }
.portlet, .aside {
    clear: both;
    border-top: 1px solid #EBEBEB;
    padding: 1em 0;
    }
.top-portlet {
    padding: 0 0 0.5em 0;
    margin: 0 0 1em;
    }
.full-page-width {
    z-index: 10;
    width: 131%;
    }
/* The content is already full width; the content should remain full width. */
.full-page-width .full-page-width {
    width: 100%;
    }
.warning.message {
    margin-top: 17px;
    }
