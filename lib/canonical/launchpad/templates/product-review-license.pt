<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="view/macro:page/onecolumn"
  i18n:domain="launchpad"
>

<body>

<div metal:fill-slot="main">

  <div style="margin-bottom: 1em">
    This project was registered by <a tal:attributes="href
    context/owner/fmt:url" tal:content="context/owner/unique_displayname" />
    <a
      tal:condition="context/owner/preferredemail"
      tal:attributes="href string:mailto:${context/owner/preferredemail/email}"
      tal:content="string: &lt;${context/owner/preferredemail/email}&gt;" /> on
    <span tal:replace="context/datecreated/fmt:datetime" />
  </div>


  <div metal:use-macro="context/@@launchpad_form/form">
    <div metal:fill-slot="extra_info">
      <input type="hidden" name="next_url"
        tal:attributes="value view/next_url" />
      <table class="listing" id="license-listing">
        <thead>
          <tr>
            <th>Licenses</th>
            <th>Description of additional licenses</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td style="white-space: nowrap">
              <tal:licenses repeat="license context/licenses">
                <tal:license tal:replace="license/title">
                  GPL
                </tal:license>
                <br/>
              </tal:licenses>
              <tal:none condition="not:context/licenses">None specified
              </tal:none>
            </td>
            <td tal:content="structure context/license_info/fmt:text-to-html"/>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

</body>
</html>
