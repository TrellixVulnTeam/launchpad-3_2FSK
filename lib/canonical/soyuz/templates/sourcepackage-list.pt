<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="soyuz">

<head>
  <!-- Plone style sheet -->
  <style type="text/css" media="screen">@import url(plone.css);</style>
  <!-- Rosetta style sheet -->
  <style type="text/css" media="screen">@import url(rosetta.css);</style>

</head>

<body>

<div metal:fill-slot="main">
<h1>Source Packages !!!!!</h1>
<!-- <div tal:on-error="python: error.type, error.value.args"> -->

  <table class="listing">
    <tr>
      <th>Packages</th>
    </tr>
    <tr tal:repeat="release context">
      <tal:dummy tal:define="package release/sourcepackage">
      <td>
        <a tal:content="package/title"
           tal:attributes="href package/name">
	  distribution name
	    </a>
      </td>
      </tal:dummy>
    </tr>
  </table>    

  <p> <a href="+search"> Search</a> in <spam tal:content="context/release/title">RELEASE</spam>
<!-- </div> -->
</div>

</body>

</html>
