<ul tal:define="team_url context/fmt:url">
  <li class="edit" tal:condition="context/required:launchpad.Edit">
    <a tal:attributes="href string:${team_url}/+edit">Edit Team Details</a>
  </li>
  <li class="mail" tal:condition="context/required:launchpad.Edit">
    <a tal:attributes="href string:${team_url}/+editemail">Edit Contact Address</a>
  </li>
  <li class="people">
    <a tal:attributes="href string:${team_url}/+members">Members</a>
  </li>
  <li class="translations">
    <a tal:attributes="href string:${team_url}/+translations">Translations</a>
  </li>
  <li class="packages">
    <a tal:attributes="href string:${team_url}/+packages">Packages</a>
  </li>
  <tal:x condition="nothing">
  <li class="info">
    <a tal:attributes="href string:${team_url}/+bounties">Bounties</a>
  </li>
  </tal:x>
</ul>
