<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="view/macro:page/pillarindex"
  i18n:domain="launchpad">
<body>
  <h1 metal:fill-slot="heading">Projects in Launchpad</h1>
  <div metal:fill-slot="main" tal:define="batch view/all_batched">
    <h1>All projects registered</h1>
    <tal:navigation replace="structure batch/@@+navigation-links-upper" />
      <div id="product-listing">
        <tal:product repeat="product batch/currentBatch"
                     replace="structure product/@@+listing-detailed" />
      </div>
    <tal:navigation replace="structure batch/@@+navigation-links-lower" />
  </div>
</body>
</html>
