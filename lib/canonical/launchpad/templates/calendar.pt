<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en" lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="launchpad">
  <head>
    <title metal:fill-slot="title">
      <span tal:replace="context/title">Calendar Title</span>
    </title>
    <!-- Plone style sheet -->
    <style type="text/css" media="screen">@import url(plone.css);</style>
  </head>

  <body>

    <div metal:fill-slot="main">
      <div id="content" class="documentEditable">
        <ul class="contentViews">
          <li id="contentview-day">
            <a href="today">Day</a>
          </li>
          <li id="contentview-week">
            <a href="this-week">Week</a>
          </li>
          <li id="contentview-month">
            <a href="this-month">Month</a>
          </li>
          <li id="contentview-year">
            <a href="this-year">Year</a>
          </li>
        </ul>
        <div class="documentContent">
          <h1><span tal:replace="context/title">Calendar Title</span></h1>
        </div>
      </div>
    </div>

  </body>
</html>
