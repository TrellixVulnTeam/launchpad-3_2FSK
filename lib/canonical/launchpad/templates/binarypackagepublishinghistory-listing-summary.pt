<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  omit-tag="">

  <tal:comment condition="nothing">
    <!-- XXX: cprov 20071126
      The template should be kept in sync with its respective binary
      replacement (sourcepackagepublishinghistory-listing-summary.pt).
      In the future we should be able to refactor both in a single
      template macro, see bug #153885.
    -->
  </tal:comment>

  <tr>
    <td style="white-space: nowrap"><a href="#"
        tal:attributes="onClick string:return toggleExpandableTableRow('pub${context/id}');"
        ><img src="/@@/treeCollapsed" alt=""
                tal:attributes="id string:pub${context/id}-arrow" /></a>
    <td tal:content="context/datecreated/fmt:datetime">XXX</td>
    <td tal:content="context/status/name">PUBLISHED</td>
    <td>
      <a tal:content="context/distroarchseries/displayname"
         tal:attributes="href context/distroarchseries/fmt:url"
      >warty i386</a>
    </td>
    <td tal:content="context/pocket/name">RELEASE</td>
    <td tal:content="context/component/name">main</td>
    <td tal:content="context/section/name">web</td>
    <td>
      <a tal:content="context/distroarchseriesbinarypackagerelease/version"
         tal:attributes="href context/distroarchseriesbinarypackagerelease/fmt:url"
      >2.0.39</a>
    </td>
  </tr>
  <tr tal:attributes="id string:pub${context/id}" style="display: none">
    <td colspan="8">
      <tal:block replace="structure context/@@+record-details" />
    </td>
  </tr>


</tal:root>
