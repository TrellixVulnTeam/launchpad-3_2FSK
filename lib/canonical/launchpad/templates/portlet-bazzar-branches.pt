<div class="portlet">
    <h4>Bazaar Branches:</h4>

    <div class="portletBody">
        <div class="portletContent even">

        <table class="rowNavigation" border="0">
            <tr tal:repeat="branch context/branches"
                tal:attributes="onclick string:window.location='/bazaar/branches/${branch/id}';">
                <td tal:content="branch/title">bug title</td>
            </tr>
        </table>

        <ul><li class="info">
        <a tal:attributes="href string:/bazaar/projects/${context/project/name}/${context/name}"
            title="See all branches for this product">
            More...
        </a>
        </li></ul>

        </div>
                
    </div>
</div>
