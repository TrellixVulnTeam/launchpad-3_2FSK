<div
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  tal:condition="not:exists:body"
  id="navigation">

  <div id="context_navigation_menu">
    [&nbsp;<tal:link repeat="link context/menu:navigation_list">
       <tal:link-content replace="structure link/@@+link">
        Link
       </tal:link-content>
     </tal:link>
    &nbsp;]<br />
  </div>
  <div id="view_navigation_menu">
    [&nbsp;<tal:link repeat="link view/menu:navigation_list">
       <tal:link-content replace="structure link/@@+link">
        Link
       </tal:link-content>
     </tal:link>
    &nbsp;]<br />
  </div>
</div>
