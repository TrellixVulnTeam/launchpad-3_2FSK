<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="launchpad">

  <facet facet="overview">

    <!-- BinaryPackagename -->
    <class class="canonical.launchpad.database.BinaryPackageName">
        <allow interface="canonical.launchpad.interfaces.IBinaryPackageName" />
    </class>

    <utility
      name="BinaryPackageName"
      component="canonical.launchpad.database.binarypackagename.BinaryPackageNameVocabulary"
      provides="zope.schema.interfaces.IVocabularyFactory"
      />

    <!-- BinaryPackageNameSet -->
    <securedutility
        class="canonical.launchpad.database.BinaryPackageNameSet"
        provides="canonical.launchpad.interfaces.IBinaryPackageNameSet">
        <allow interface="canonical.launchpad.interfaces.IBinaryPackageNameSet" />
    </securedutility>

    <class class="canonical.launchpad.database.BinaryPackageNameSet">
        <allow interface="canonical.launchpad.interfaces.IBinaryPackageNameSet" />
    </class>

  </facet>
</configure>
