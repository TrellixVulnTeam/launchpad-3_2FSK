<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="launchpad">

<body>

<metal:leftportlets fill-slot="portlets_one">
  <div tal:replace="structure context/@@+portlet-details" />
</metal:leftportlets>

<div metal:fill-slot="main">

  <h1>Build log</h1>

     <tal:block condition="context/buildlog">
        <a tal:attributes="href context/buildlog/url"
           tal:content="context/buildlog/filename">BUILDLOG</a>
     </tal:block>

     <tal:block condition="not: context/buildlog">
        <p> No buildlog for <span tal:replace="context/title">BUILD</span></p>
     </tal:block>

</div>
</body>
</html>
