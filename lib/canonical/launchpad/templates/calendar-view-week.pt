<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en" lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="launchpad">
  <head>
    <title metal:fill-slot="title">
      <span tal:replace="context/calendar/title">Calendar Title</span> -
      <span tal:replace="context/datestring">Time</span>
    </title>
    <!-- Plone style sheet -->
    <style type="text/css" media="screen">@import url(plone.css);</style>
  </head>

  <body>

    <div metal:fill-slot="main">
      <div id="content" class="documentEditable">
        <ul class="contentViews">
          <li id="contentview-day">
            <a tal:attributes="href context/dayViewURL">Day</a>
          </li>
          <li id="contentview-week" class="selected">
            <a tal:attributes="href context/weekViewURL">Week</a>
          </li>
          <li id="contentview-month">
            <a tal:attributes="href context/monthViewURL">Month</a>
          </li>
          <li id="contentview-year">
            <a tal:attributes="href context/yearViewURL">Year</a>
          </li>
        </ul>
        <div class="documentContent">
          <h1>
            <span tal:replace="context/calendar/title">Calendar Title</span> -
            <span tal:replace="context/datestring">Time</span>
          </h1>


          <a tal:attributes="href context/prevURL">&lt;- Previous</a>
          <a tal:attributes="href context/nextURL">Next -&gt;</a>

      <table border="1">
        <tr>
          <td>
            <h2><a tal:attributes="href context/monday/dayURL"
                   tal:content="context/monday/dayname">Monday</a></h2>
          </td>
          <td>
            <h2><a tal:attributes="href context/tuesday/dayURL"
                   tal:content="context/tuesday/dayname">Tuesday</a></h2>
          </td>
        </tr>
        <tr>
          <td>
            <h2><a tal:attributes="href context/wednesday/dayURL"
                   tal:content="context/wednesday/dayname">Wednesday</a></h2>
          </td>
          <td>
            <h2><a tal:attributes="href context/thursday/dayURL"
                   tal:content="context/thursday/dayname">Thursday</a></h2>
          </td>
        </tr>
        <tr>
          <td rowspan="2">
            <h2><a tal:attributes="href context/friday/dayURL"
                   tal:content="context/friday/dayname">Friday</a></h2>
          </td>
          <td>
            <h2><a tal:attributes="href context/saturday/dayURL"
                   tal:content="context/saturday/dayname">Saturday</a></h2>
          </td>
        </tr>
        <tr>
          <td>
            <h2><a tal:attributes="href context/sunday/dayURL"
                   tal:content="context/sunday/dayname">Sunday</a></h2>
          </td>
        </tr>
      </table>
        </div>
      </div>
    </div>

  </body>
</html>
