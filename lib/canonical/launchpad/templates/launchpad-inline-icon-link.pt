<tal:link-enabled
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  condition="context/enabled"
><a
  tal:attributes="
    class string:menu-link-${context/name};
    href context/url;
    title context/summary;"
><img
   tal:attributes="
     src context/icon_url;
     alt context/text" /></a></tal:link-enabled>
