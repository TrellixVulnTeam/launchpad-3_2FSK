<metal:page define-macro="master">
<metal:doctype define-slot="doctype">
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
</metal:doctype>
<metal:block define-slot="top_slot" />
<html xmlns="http://www.w3.org/1999/xhtml"
      xml:lang="en"
      lang="en"
      tal:attributes="lang default_language|default;
                      xml:lang default_language|default;">
  <head>
    <metal:block define-slot="title"></metal:block>
    <style type="text/css" media="all">@import url(http://www.ubuntulinux.org/ploneColumns.css);</style>

    <style type="text/css" media="screen"> @import url(http://www.ubuntulinux.org/plone.css);</style> 

    <style type="text/css" media="all">@import url(http://www.ubuntulinux.org/ploneCustom.css);</style>
  </head>

  <body>
    <div id="visual-portal-wrapper">

      <div id="portal-top">

        <a class="hiddenStructure"
           href="http://www.ubuntulinux.org/#documentContent">Skip to content</a>

        <ul id="portal-siteactions">

    
          <li id="siteaction-small_text"><a
          href="javascript:setActiveStyleSheet('Small Text', 1);"
          title="Small Text">Small Text</a></li>
          <li id="siteaction-normal_text"><a
          href="javascript:setActiveStyleSheet('', 1);"
          title="Normal Text">Normal Text</a></li>
          <li id="siteaction-large_text"><a
          href="javascript:setActiveStyleSheet('Large Text', 1);"
          title="Large Text">Large Text</a></li>
    
       </ul>

       <div id="portal-searchbox">
         <form name="searchform"
         action="http://www.ubuntulinux.org/search">
           <label for="searchGadget" class="hiddenStructure">Search</label>

        <input id="searchGadget" name="SearchableText"
               type="text" size="20" alt="Search"
               title="Search" accesskey="s"
               class="visibility:visible" tabindex="30001" />
        <input class="searchButton" type="submit"
               value="Search" accesskey="s" tabindex="30002" />
         </form>
       </div>

       <h1 id="portal-logo">
         <a href="http://www.ubuntulinux.org">Ubuntu Linux</a>
       </h1>

       <div id="portal-skinswitcher">

    </div>

          <ul id="portal-globalnav"
              tal:define="path_info request/PATH_INFO">

            <li class="selected"
                tal:condition="python:path_info is 'http://www.ubuntulinux.org/'">
              <a href="http://www.ubuntulinux.org/">Ubuntu</a>
            </li>
            <li class="plain"
                tal:condition="python:not path_info is 'http://www.ubuntulinux.org/'">
              <a href="http://www.ubuntulinux.org/">Ubuntu</a>
            </li>

            <li class="selected"
                tal:condition="python:'/community' in path_info">
              <a href="http://www.ubuntulinux.org/community">Community</a>
            </li>
            <li class="plain"
                tal:condition="python:'/community' not in path_info">
              <a href="http://www.ubuntulinux.org/community">Community</a>
            </li>

            <li class="selected"
                tal:condition="python:'/support' in path_info">
              <a href="http://www.ubuntulinux.org/support">Support</a>
            </li>
            <li class="plain"
                tal:condition="python:'/support' not in path_info">
              <a href="http://www.ubuntulinux.org/support">Support</a>
            </li>


          </ul>

     <ul id="portal-personaltools">

       <li class="portalNotLoggedIn">
          You are not logged in
       </li>
    
       <li>
          <a href="https://www.ubuntulinux.org/login_form"
             accesskey="u">
             Log in
          </a>
       </li>

     </ul>
      <div id="portal-breadcrumbs">
        You are here:
        <span>

            
        </span>

        <span>

            <span>
                <a href="https://ubuntulinux.org/">ubuntulinux</a>
                <span class="breadcrumbSeparator"> &raquo; </span>
            </span>
        </span>
        <span>

            <span>
                <a href="https://ubuntulinux.org/forgottenpassword/">forgottenpassword</a>
            </span>
        </span>
      </div>

      <div class="visualClear"></div>

      <table id="portal-columns">
        <tbody>
          <tr>

            <!-- start of the left (by default at least) column -->
            <td id="portal-column-one"
                metal:define-slot="column_one">
              <div class="visualPadding">
                <metal:portlets define-slot="portlets_one">
                
                </metal:portlets>
                &nbsp;
              </div>
            </td>
            <!-- end of the left (by default at least) column -->

            <td id="portal-column-content">
              <div id="region-content">
                  <metal:main define-slot="main">
                  <p>Main content area.</p>
                  </metal:main>
              </div>
            </td>

            <!-- start of right (by default at least) column -->
            <td id="portal-column-two"
                metal:define-slot="column_two">
              <div class="visualPadding">
                <metal:portlets define-slot="portlets_two">
                
                </metal:portlets>
                &nbsp;
              </div>
            </td>
            <!-- end of the right (by default at least) column -->

          </tr>
        </tbody>
      </table>
      <!-- end column wrapper -->

      <div class="visualClear">&nbsp;</div>

      <metal:block>
        <div id="portal-footer">
            &copy 
            2004
            <a href="http://www.canonical.com"> Canonical Ltd.</a> 
            Ubuntu and Canonical are trademarks of Canonical Ltd. <br />
            <a href=" http://www.ubuntulinux.org/feedback">Feedback</a> &mdash; 
            <a href=" http://www.ubuntulinux.org/legal">Legal</a> &mdash; 
            <a href=" http://www.ubuntulinux.org/sitemap">Site Map</a>
        </div>

   <div id="portal-colophon">

      <!--

        Please keep the Plone Powered button (or a textual link to us) if you use
        Plone on your site. It's a nice token of gratitude, and we appreciate your
        help in promoting the Plone name.

        Plone is powered by the combined forces of Zope and CMF, two absolutely
        great systems made by Zope Corporation (http://zope.com) and they in turn
        are based on the best programming language in the world - Python
        (http://www.python.org). We owe these guys a lot, thanks for making Plone
        possible!

      -->

      <a href="http://plone.org">
        <img src="http://www.ubuntulinux.org/plone_powered.gif"
      height="15" width="80" alt="Powered by Plone"
      title="This Plone site was built using the Plone Content Management System. Click for more information." />
      </a>

      

      <p class="discreet">
        This site conforms to the following standards:
      </p>

      <ul>

        <li>
          <a href="http://www.section508.gov">
            <img src="http://www.ubuntulinux.org/colophon_sec508.gif"
          height="15" width="80" alt="Section 508"
          title="This Plone site conforms to the US Government Section 508 Accessibility Guidelines." />
          </a>
        </li>
        <li>
          <a href="http://www.w3.org/WAI/WCAG1AA-Conformance"
      title="Explanation of WCAG Conformance">
            <img src="http://www.ubuntulinux.org/colophon_wai-aa.gif"
          height="15" width="80" alt="WCAG"
          title="This Plone site conforms to the W3C-WAI Web Content Accessibility Guidelines." />
          </a>

        </li>
        <li>
          <a href="http://validator.w3.org/check/referer">
            <img src="http://www.ubuntulinux.org/colophon_xhtml.png"
          height="15" width="80" alt="Valid XHTML"
          title="This Plone site is valid XHTML." />
          </a>
        </li>
        <li>
          <a href="http://jigsaw.w3.org/css-validator/check/referer">
            <img src="http://www.ubuntulinux.org/colophon_css.png"
          height="15" width="80" alt="Valid CSS"
          title="This Plone site was built with valid CSS." />

          </a>
        </li>
        <li>
          <a href="http://plone.org/browsersupport">
            <img src="http://www.ubuntulinux.org/colophon_anybrowser.png"
          height="15" width="80" alt="Usable in any browser"
          title="This Plone site is usable in any web browser." />
          </a>
        </li>
      </ul>

    </div>


      </metal:block>
  </body>
</html>
</metal:page>
