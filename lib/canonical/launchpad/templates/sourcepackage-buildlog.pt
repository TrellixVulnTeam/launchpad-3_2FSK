<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="launchpad">

<body>

<div metal:fill-slot="main">


    <h1> 
     <span tal:content="context/sourcepackage/name">
	TITLE </span>
    </h1>

    <fieldset class="dummy" 
         tal:repeat="sourcepackagerelease context/releases">
      <legend>
        <span tal:content="string:${context/sourcepackage/name}-${sourcepackagerelease/version}"/> Build Data
      </legend>
      <ul>
      <li tal:repeat="arch context/archs" 
          tal:define="name context/sourcepackage/name; 
                      version sourcepackagerelease/version">
	  <a tal:attributes="href string:${version}/${arch}/+buildlog" 
	     tal:content="string:${name}-${version} (${arch})">
	  BUILDLOG</a></li>
      <ul>
    </fieldset>    
    
</div>
</body>
</html>
 
