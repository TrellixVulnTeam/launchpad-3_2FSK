<html metal:use-macro="view/macro:page/pillarindex" i18n:domain="launchpad">
<body>
  <h1 metal:fill-slot="heading">Projects in Launchpad</h1>
  <div metal:fill-slot="main" tal:define="batch view/all_batched">
    <h1>All projects registered</h1>
    <tal:navigation replace="structure batch/@@+navigation-links-upper" />
    <div tal:repeat="product batch/currentBatch"
         tal:replace="structure product/@@+listing-detailed" />
    <tal:navigation replace="structure batch/@@+navigation-links-lower" />
  </div>
</body>
</html>
