<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="context/@@main_template/master"
      i18n:domain="rosetta">

<head>
    <!-- Plone style sheet -->
    <style type="text/css" media="screen">@import url(plone.css);</style>
    <!-- Rosetta style sheet -->
    <style type="text/css" media="screen">@import url(rosetta.css);</style>
</head>

<body>

<div metal:fill-slot="main">

    <h1><span tal:content="context/distribution/title">TITLE</span> 
    Distribution Overview</h1>

   <table width="80%">
   <tr> <td>
     <h4>Latest Posts</h4>
   </td><td>
     <h4>Related Links</h4>
   </td></tr>
   <tr><td>
    <table>
    <tr><td><i>Hello World</i></td><td></td></tr>
    <tr><td></td><td><a href="../../people/1">Lamont</a></td></tr>
    <tr><td><i>Soyuz Rocks</i></td><td></td></tr>
    <tr><td></td><td><a href="../../people/1">Cprov</a></td></tr>
    <tr><td><i>Launchpad burns</i></td><td></td></tr>
    <tr><td></td><td><a href="../../people/1">Kiko</a></td></tr>
    </table>
   </td><td>
   <ul> <a href="src"><span tal:content="context/distribution/title">
   TITLE</span> Sourcepackages</a></ul> 
   <ul> <a href="bin"><span tal:content="context/distribution/title">
   TITLE</span> Binarypackages</a></ul>  
   <ul> <a href="team"><span
   tal:content="context/distribution/title">TITLE</span> 
   Team </a> </ul> 
   <ul> <a href="+translations"><span
   tal:content="context/distribution/title">TITLE</span> 
   Translations </a> </ul> 
   <ul> <a href="+bugs"><span
   tal:content="context/distribution/title">TITLE</span> 
   Bugs</a> </ul> 
   </td></tr>
   <tr><td>
   <h3><span tal:content="context/distribution/title">TITLE</span> Distribution
      Description</h3> 
   </td></tr>
   <tr><td>
   <div>
   <p><span tal:content="context/distribution/description">DESCRIPTION</span>
   </div>
   </td></tr>
   <tr><td>
    <p> <a href="+edit">Edit <span
    tal:content="context/distribution/title">
    TITLE</span> Information</a>   
   </td></tr>  
   <tr><td>
   <table class="listing">
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Description</th>
      <th>Version</th>
      <th>Packages</th>
      <th>Status</th>
      <th>Date Released</th>
      <th>Parent Release</th>
      <th>Owner</th>
    </tr>
    <tr tal:repeat="release context/releases">
      <td><a tal:content="release/name"
	   tal:attributes="href string:releases/${release/name}">
	   distribution name</a>
      </td>
      <td tal:content="release/title">TITLES</td>
      <td tal:content="release/description">DESCRIPTION</td>
      <td tal:content="release/version">VERSION</td>
      <td>
       <table>
       <tr>
       <th><a tal:attributes="href string:src/${release/name}">SRC</a></th>
       <th><a tal:attributes="href string:bin/${release/name}">BIN</a></th>
       </tr>
       </table>
      </td>
      <td tal:content="release/releasestate">RELEASESTATE</td>
      <td tal:content="release/datereleased">DATERELEASED</td>      
      <td>xxxx</td>          
      <td tal:content="release/owner/displayname">OWNER</td>            
    </tr>
    </table>    
   </td></tr>
   <tr><td>
   <p><a href="releases/+new">Create a new <span
        tal:content="context/distribution/title">TITLE</span>
	 Release</a>
   </td></tr>
   <tr><td>
   <p><a href="releases">See Current <span
	tal:content="context/distribution/title">TITLE</span> 
	Releases</a><b>(DEPRECATED!!)</b> 
  </td></tr>
  </table>
 
</div>

</body>
</html>
