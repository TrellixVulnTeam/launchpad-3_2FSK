<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  omit-tag="">

<html
  xmlns="http://www.w3.org/1999/xhtml"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  xml:lang="en"
  lang="en"
  dir="ltr"
  metal:use-macro="context/@@main_template/master"
  i18n:domain="launchpad"
>

<body>

<metal:portlets fill-slot="portlets">
</metal:portlets>

<div metal:fill-slot="main">

  <div metal:use-macro="context/@@launchpad_form/form">

    <h1 metal:fill-slot="heading">Link an existing branch to this series</h1>

  </div>
  <strong id="cannot-link-explanation" tal:condition="not:view/not_import">
    Cannot change link because this series is linked to an import.
  </strong>

</div>
</body>
</html>
</tal:root>
