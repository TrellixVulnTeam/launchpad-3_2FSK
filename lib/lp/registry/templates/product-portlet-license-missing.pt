<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  omit-tag="">

<div class="portlet" id="portlet-missing-license"
     tal:condition="not: context/licenses">
  <h2>
    Missing license information
  </h2>
  <div>
    <img style="float:left;" alt="Expiration warning"
      src="/@@/expiration-large" />
    This project needs to have its licensing information entered.
    <br />Select the appropriate license checkboxes from
    <a tal:content="structure context/menu:overview/edit/fmt:link-icon" />.
  </div>
</div>
</tal:root>
