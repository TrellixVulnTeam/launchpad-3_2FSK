<tal:tag condition="view/update"/>
<html
    xmlns="http://www.w3.org/1999/xhtml"
    xml:lang="en"
    lang="en"
    metal:use-macro="context/@@main_template/master"
    i18n:domain="launchpad">

<body>

<div metal:fill-slot="main">

      <div>
        <img alt="" src="/@@/blueprint" />
        <a tal:attributes="href context/fmt:url" tal:content="context/title" />
      </div>

  <div metal:use-macro="context/@@launchpad_editform/editform">

    <p metal:fill-slot="extra_info">
      The priority should reflect the views of the coordinating team of
      <span tal:replace="context/target/displayname">Ubuntu</span>.
    </p>

  </div>

</div>
</body>
</html>
