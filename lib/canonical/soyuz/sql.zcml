  </content>

  <content class="canonical.soyuz.sql.PersonApp">
    <allow interface="canonical.launchpad.interfaces.IPersonApp"/>
    <require
     permission="zope.Public"
     set_attributes="email wiki irc jabber gpg archuser"
     />	
    <implements interface="zope.app.container.interfaces.IItemContainer"/>
  </content>

  <content class="canonical.launchpad.database.EmailAddress">
    <allow interface="canonical.launchpad.interfaces.ISoyuzEmailAddress"/>
    <require
     permission="zope.Public"
     set_attributes="email status"
     />	
    <implements interface="zope.app.container.interfaces.IItemContainer"/>
  </content>

  <content class="canonical.launchpad.database.GPGKey">
    <allow interface="canonical.launchpad.interfaces.IGPGKey"/>
    <require
     permission="zope.Public"
     set_attributes="keyid fingerprint pubkey revoked"
     />
    <implements interface="zope.app.container.interfaces.IItemContainer"/>
  </content>

  <content class="canonical.launchpad.database.ArchUserID">
