<?xml version="1.0" encoding="UTF-8"?>
<metal:entry metal:define-macro="feed-entry"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:lp="http://api.launchpad.net/schema"
    xmlns:metal="http://xml.zope.org/namespaces/metal">
    <title tal:content="entry/title" />
    <link rel="alternate" tal:attributes="href entry/URL" />
    <published tal:content="entry/date_published" />
    <updated tal:content="entry/date_updated" />
    <id tal:content="entry/id_">http://api.launchpad.net/bugs/NNN#description</id>
    <author>
        <name tal:content="entry/author/displayname" />
    </author>
    <content tal:content="entry/content" type="html" />
</metal:entry>
