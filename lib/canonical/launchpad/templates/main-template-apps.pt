<!--
 Current exact: <strong><span>Bugs</span></strong>
 Current child: <strong><a>Bugs</a></strong>
 Other enabled: <span><a>Code</a></span>
Other disabled: <span></span>Code</span></span>
-->
<tal:root
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
>

<div
  tal:condition="view/macro:pagehas/applicationtabs"
  tal:define="facetmenu CONTEXTS/menu:facet"
  id="lp-apps"
>
  <tal:facet repeat="link facetmenu">
    <strong
      tal:condition="link/selected"
      tal:attributes="title link/summary; class link/name"
    >
      <span
        tal:condition="not:link/linked"
        tal:content="structure link/escapedtext"
      />
      <a
        tal:condition="link/linked"
        tal:attributes="href link/url"
        tal:content="structure link/escapedtext"
      />
    </strong>
    <span
      tal:condition="not:link/selected"
      tal:attributes="title link/summary; class link/name"
    >
      <span
        tal:condition="not:link/linked"
        tal:content="structure link/escapedtext"
      />
      <a
        tal:condition="link/linked"
        tal:attributes="href link/url"
        tal:content="structure link/escapedtext"
      />
    </span>
  </tal:facet>
</div>

</tal:root>