<span tal:attributes="id view/id">
    <span class="yui-activator-data-box">
    <tal:attribute replace="structure view/default_html"/>
    </span>
    <button class="lazr-btn yui-activator-act yui-activator-hidden">
      Edit
    </button>
    <div class="yui-activator-message-box yui-activator-hidden"/>
</span>
<script tal:condition="context/required:launchpad.Edit"
        tal:content="structure string:
YUI().use('lp.picker', function(Y) {
    if (Y.UA.ie) {
        return;
    }

    Y.lp.picker.addPickerPatcher(
        '${view/vocabulary}',
        '${view/resource_uri}',
        '${view/json_attribute_uri_base}',
        '${view/json_attribute}',
        '${view/id}',
        ${view/config});
});
"/>
